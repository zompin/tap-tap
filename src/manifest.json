{
  "manifest_version": 3,
  "name": "Tap-Tap",
  "version": "0.2",
  "description": "Site blocker",
  "background": {
    "scripts": ["js/main.js"],
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "rokot@tap-tap"
    }
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["webNavigation", "storage"]
}
